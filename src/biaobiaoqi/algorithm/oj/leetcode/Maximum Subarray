public class Solution {
    public int maxSubArray(int[] A) {
        int[] s = new int[A.length];
        int max = Integer.MIN_VALUE;
        for (int i = 0; i < A.length; i ++) {
            s[i] = A[i];
            if (i > 0 && s[i - 1] > 0)
                s[i] += s[i - 1];
            
            if (max < s[i])
                max = s[i];
        }
        return max;
    }
}
