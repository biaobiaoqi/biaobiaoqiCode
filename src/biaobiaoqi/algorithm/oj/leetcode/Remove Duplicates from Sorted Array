public class Solution {
    public int removeDuplicates(int[] A) {
       int n = A.length;
       int index = 0;
       if (n == 0) return 0;
       
       for (int i = 1; i != n; i ++) {
           if (A[index] != A[i]) {
               A[++index] = A[i];
           }
       }
       
       return index + 1;
    }
}